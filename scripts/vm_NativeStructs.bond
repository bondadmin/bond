native
{
	struct Vector3
	{
		int X;
		int Y;
		int Z;
		int Dot(const Vector3 *other) const;
	};
}

int GetVector3X(const Vector3 *v3)
{
	return v3->X;
}

int GetVector3Y(const Vector3 *v3)
{
	return v3->Y;
}

int GetVector3Z(const Vector3 *v3)
{
	return v3->Z;
}

int DotVector3(const Vector3 *a, const Vector3 *b)
{
	return a->Dot(b);
}
