int While(int a)
{
	int b = 0;
	while (b < a)
	{
		b = b + 1;
	}
	return b;
}

int For(int a)
{
	int b = 0;
	for (int i = 0; i < a; ++i)
	{
		b = b + 1;
	}
	return b;
}

int NestedLoopsWithJumps(int a, int b)
{
	int c = 0;

	for (int i = 0; ;++i)
	{
		if (i == 3)
		{
			continue;
		}

		for (int j = 0; ;++j)
		{
			if (j == 6)
			{
				continue;
			}

			c += (i * j);

			if (j == b)
			{
				break;
			}
		}

		if (i == a)
		{
			break;
		}
	}

	return c;
}
