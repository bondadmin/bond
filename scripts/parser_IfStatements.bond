void TheFunction(int a, int b)
{
	// Empty then.
	if (a < 1);

	// Single statement then.
	if (a < 2)
		b = 2;

	// Compound statement then.
	if (a < 3)
	{
		b = 3;
	}

	// Else.
	if (a < 4)
		b = 4;
	else
		b = 5;

	// Cascaded.
	if (a < 6)
		b = 6;
	else if (a < 7)
		b = 7;
	else
		b = 8;

	// Nested.
	if (a < 9)
		if (a < 10)
			b = 10;
		else
			b = 11;
	else b = 12;
}
